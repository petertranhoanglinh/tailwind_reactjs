        "menu-desc": "Retrieves bonus analysis details for a specific company, user, and year.",
        "input": {
            "description": "The bonusAnalysicGird API retrieves a detailed breakdown of bonuses received by a user for a given year, categorized by different bonus types.",
            "url": "api/bonus/bonusAnalysicGird",
            "method": "GET",
            "content-type": "application/json",
            "parameters": [
                {
                    "name": "comId",
                    "type": "text",
                    "required": "required",
                    "length": 8,
                    "remarks": "Company ID associated with the user."
                },
                {
                    "name": "lang",
                    "type": "text",
                    "required": "required",
                    "length": 3,
                    "remarks": "Language code (e.g., 'US', 'VN', 'KR' , 'JP')."
                },
                {
                    "name": "userid",
                    "type": "text",
                    "required": "required",
                    "length": 10,
                    "remarks": "User ID for whom the bonus data is retrieved."
                },
                {
                    "name": "year",
                    "type": "text",
                    "required": "required",
                    "length": 4,
                    "remarks": "The year for which the bonus analysis is retrieved (format: YYYY)."
                }
            ]
        },
        "output": {
            "description": "The bonusAnalysicGird API returns a JSON object containing bonus-related information for a specific user and year, grouped by month.",
            "success": {
                "status": "success",
                "message": "Query executed successfully",
                "fields": [
                    {
                        "name": "giveDate",
                        "type": "text",
                        "remarks": "The bonus distribution month in YYYYMM format."
                    },
                    {
                        "name": "amt1 - amt15",
                        "type": "number",
                        "remarks": "Amount pay name 1 -> 15 (Visibility controlled by Bonus settings)"
                    },
                    {
                        "name": "totalAmt",
                        "type": "number",
                        "remarks": "Total bonus amount for the corresponding month."
                    }
                ]
            },
            "error": [
                {
                    "error-code": "401",
                    "message": "Unauthorized access."
                },
                {
                    "error-code": "400",
                    "message": "Invalid input parameters."
                },
                {
                    "error-code": "500",
                    "message": "Internal server error."
                }
            ]
        }
