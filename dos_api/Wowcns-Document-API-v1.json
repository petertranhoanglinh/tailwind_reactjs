[
    {
        "menu-parent": "Bonus",
        "menu-name": "SearchBonusDetail",
        "menu-desc": "Retrieves bonus benefit details based on the given criteria.",
        "input": {
            "description": "User must be authenticated to search for bonus benefit details.",
            "url": "api/bonus/BenefitDetailInquiry/search",
            "method": "POST",
            "content-type": "application/json",
            "body": {
                "fields": [
                    {
                        "name": "comId",
                        "type": "text",
                        "required": "required",
                        "length": 8,
                        "remarks": "Company ID"
                    },
                    {
                        "name": "userid",
                        "type": "text",
                        "required": "required",
                        "length": 10,
                        "remarks": "User ID"
                    },
                    {
                        "name": "type",
                        "type": "text",
                        "required": "required",
                        "length": 1,
                        "remarks": "1 (retrieve data from Pay_Calc_1 for monthly summary), 2 (retrieve data from Pay_Calc_2 for yearly summary)"
                    },
                    {
                        "name": "date1",
                        "type": "text",
                        "required": "optional",
                        "length": 10,
                        "remarks": "Start date for search with format YYYYMMDD (Ex: 20251230)"
                    },
                    {
                        "name": "date2",
                        "type": "text",
                        "required": "optional",
                        "length": 10,
                        "remarks": "End date for search with format YYYYMMDD (Ex: 20251230)"
                    },
                    {
                        "name": "page",
                        "type": "int",
                        "required": "Optional",
                        "length": 100,
                        "remarks": "number of pages of list(default 0)",
                        "example": 0
                    },
                    {
                        "name": "len",
                        "type": "int",
                        "required": "Optional",
                        "length": 100,
                        "remarks": "number of length of list(default 10)",
                        "example": 10
                    }
                ]
            }
        },
        "output": {
            "description": "Information about the bonus benefit details.",
            "success": {
                "status": "Ok",
                "message": "Bonus benefit details retrieved successfully",
                "fields": [
                    {
                        "name": "paydate",
                        "type": "text",
                        "required": "required",
                        "remarks": "Payment date with format YYYYMMDD (Ex: 20251230)"
                    },
                    {
                        "name": "rankName",
                        "type": "text",
                        "required": "required",
                        "remarks": "User rank name"
                    },
                    {
                        "name": "totalAmt",
                        "type": "number",
                        "required": "required",
                        "remarks": "Total amount"
                    }
                ]
            },
            "error": [
                {
                    "error-code": "401",
                    "message": "Unauthorized access."
                },
                {
                    "error-code": "400",
                    "message": "Invalid input parameters."
                },
                {
                    "error-code": "500",
                    "message": "Internal server error."
                }
            ]
        }
    },
    {
        "menu-parent": "Bonus",
        "menu-name": "BonusMonthlySearch",
        "menu-desc": "Displays bonus payment details by month based on the selected criteria for a specific user",
        "input": {
            "description": "The screen displays a 'Bonus Monthly Search' page where users can filter and search for bonus payments by selecting a date range and step type (Monthly or Yearly).",
            "url": "api/bonus/accountingInquiry/search",
            "method": "POST",
            "content-type": "application/json",
            "body": {
                "fields": [
                    {
                        "name": "comId",
                        "type": "text",
                        "required": "required",
                        "length": 8,
                        "remarks": "Company ID for the query"
                    },
                    {
                        "name": "userid",
                        "type": "text",
                        "required": "required",
                        "length": 8,
                        "remarks": "User ID for whom the data is retrieved"
                    },
                    {
                        "name": "date1",
                        "type": "text",
                        "required": "required",
                        "length": 10,
                        "remarks": "Start date (YYYYMMDD format)"
                    },
                    {
                        "name": "date2",
                        "type": "text",
                        "required": "required",
                        "length": 10,
                        "remarks": "End date (YYYYMMDD format)"
                    },
                    {
                        "name": "chkDate",
                        "type": "integer",
                        "required": "required",
                        "length": 1,
                        "remarks": "1 (summarize by month), 2 (summarize by year)"
                    },
                    {
                        "name": "page",
                        "type": "int",
                        "required": "Optional",
                        "length": 100,
                        "remarks": "number of pages of list(default 0)",
                        "example": 0
                    },
                    {
                        "name": "len",
                        "type": "int",
                        "required": "Optional",
                        "length": 100,
                        "remarks": "number of length of list(default 10)",
                        "example": 10
                    }
                ]
            }
        },
        "output": {
            "description": "Bonus monthly search results displaying payment dates and total amounts based on the selected criteria.",
            "success": {
                "status": "Ok",
                "message": "Query executed successfully",
                "fields": [
                    {
                        "name": "paydate",
                        "type": "text",
                        "remarks": "Payment date in YYYYMM format"
                    },
                    {
                        "name": "amt1 - amt15",
                        "type": "number",
                        "remarks": "Amount pay name 1 -> 15 (Visibility controlled by Bonus settings)"
                    },
                    {
                        "name": "totalAmt",
                        "type": "number",
                        "remarks": "Total amount calculated"
                    }
                ]
            },
            "error": [
                {
                    "error-code": "401",
                    "message": "Unauthorized access."
                },
                {
                    "error-code": "400",
                    "message": "Invalid input parameters."
                },
                {
                    "error-code": "500",
                    "message": "Internal server error."
                }
            ]
        }
    },
    {
        "menu-parent": "Bonus",
        "menu-name": "BonusCalendar",
        "menu-desc": "Retrieves benefit calendar details for a specific company and user.",
        "input": {
            "description": "The getBenefitCalendar API retrieves benefit-related data for a user based on the specified company, user ID, and month. This API is used to populate the benefit calendar with relevant information.",
            "url": "api/bonus/getBenefitCalendar",
            "method": "GET",
            "content-type": "application/json",
            "parameters": [
                {
                    "name": "comId",
                    "type": "text",
                    "required": "required",
                    "length": 8,
                    "remarks": "Company ID for the query"
                },
                {
                    "name": "userid",
                    "type": "text",
                    "required": "required",
                    "length": 10,
                    "remarks": "User ID for whom the data is retrieved"
                },
                {
                    "name": "yyyymm",
                    "type": "text",
                    "required": "required",
                    "length": 6,
                    "remarks": "Year and month in YYYYMM format"
                }
            ]
        },
        "output": {
            "description": "The getBenefitCalendar API is a JSON object containing benefit-related information for a specific user, company, and month. ",
            "success": {
                "status": "Ok",
                "message": "Query executed successfully",
                "fields": [
                    {
                        "name": "payDate",
                        "type": "text",
                        "remarks": "Payment date in YYYYMMDD format"
                    },
                    {
                        "name": "payDay",
                        "type": "text",
                        "remarks": "Day of the month for the payment"
                    },
                    {
                        "name": "totalAmt",
                        "type": "number",
                        "remarks": "Total amount calculated"
                    },
                    {
                        "name": "taxAmt",
                        "type": "number",
                        "remarks": "Total tax amount deducted"
                    },
                    {
                        "name": "trueAmt",
                        "type": "number",
                        "remarks": "Actual amount received after tax deductions"
                    }
                ]
            },
            "error": [
                {
                    "error-code": "401",
                    "message": "Unauthorized access."
                },
                {
                    "error-code": "400",
                    "message": "Invalid input parameters."
                },
                {
                    "error-code": "500",
                    "message": "Internal server error."
                }
            ]
        }
    },
    {
        "menu-parent": "Bonus",
